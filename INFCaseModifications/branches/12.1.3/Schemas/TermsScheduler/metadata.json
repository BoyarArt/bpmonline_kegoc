          "CH1": "//Удаление задачи и связвнных тригеров если они ранее существовали\r\nstring processName = \"onSolutionTime70PercentEnded\";\r\nstring processName1 = \"onSolutionTime50PercentEnded\";\r\nAppScheduler.RemoveJob(processName + \"Job\", processName + \"Group\");\r\n// Создание задания для запуска процесса.\r\nIJobDetail job = AppScheduler.CreateProcessJob(processName + \"Job\", processName + \"Group\", processName,\r\n    UserConnection.Workspace.Name, UserConnection.CurrentUser.Name);\r\n// Создание Cron-триггера, соответствующего графику запуска задания.\r\nITrigger trigger = new CronTriggerImpl(processName + \"Job\", processName + \"Group\", \"0 0/1 * 1/1 * ? *\");\r\n// Добавление задания и триггера в планировщик.\r\nAppScheduler.Instance.ScheduleJob(job, trigger);\r\n\r\nAppScheduler.RemoveJob(processName1 + \"Job\", processName1 + \"Group\");\r\n// Создание задания для запуска процесса.\r\nIJobDetail job1 = AppScheduler.CreateProcessJob(processName1 + \"Job\", processName1 + \"Group\", processName1,\r\n    UserConnection.Workspace.Name, UserConnection.CurrentUser.Name);\r\n// Создание Cron-триггера, соответствующего графику запуска задания.\r\nITrigger trigger1 = new CronTriggerImpl(processName1 + \"Job\", processName1 + \"Group\", \"0 0/1 * 1/1 * ? *\");\r\n// Добавление задания и триггера в планировщик.\r\nAppScheduler.Instance.ScheduleJob(job1, trigger1);\r\n\r\nreturn true;"
